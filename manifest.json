{
  "name": "KISS to-do list for Chrome",
  "description": "Very, very, very simple to-do list for Chrome",
  "author": "Mikel Larreategi",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}